# defining a dictionary
programming_dictionary = {
  "Bug": "An error in a program that prevents the program from running as expected.",
   "Function": "A piece of code that you can easily call over and over again."
    }

    
#printing an element of the dictionary
print(programming_dictionary["Bug"])

#adding an element to the dictionary
programming_dictionary["loop"]="The process of doing something again and again"
print(programming_dictionary["loop"])
print(programming_dictionary)

#adding an empty dictionary
empty_dictionary = {}

#add an element to the empty empty_dictionary
empty_dictionary["object"] = "instance of a class"
print(empty_dictionary)
